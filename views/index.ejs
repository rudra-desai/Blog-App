<%- include("partials/header") %>

<% blogs.forEach(function(blog){ %>
	<div class = "ui main text container segment">
		<p><%= blog.title %></p>
		<img src = "<%=blog.image%>"><br>
		<p><%= blog.body.substring(0, 100) %>...</p>
		<a href="/blogs/<%= blog._id %>">Read More</a>
	</div>
<% }) %>
<%- include("partials/footer") %>
